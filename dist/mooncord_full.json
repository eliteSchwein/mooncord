    "embed_meta": {
        "fileinfo": {
            "author": "${filename}",
            "color": "#0099ff",
            "buttons": [
                "printjob_start",
                "printjob_delete_request",
                "to_printlist"
            ]
        },
        "list_files": {
            "color": "#0099ff",
            "thumbnail": "printlist.png",
            "buttons": [
                "last_page_printlist",
                "next_page_printlist"],
            "selection": {
                "id": "printlist_view_printjob",
                "data": "${raw_entries}"
            }
        },
        "info": {
            "color": "#0099ff",
            "thumbnail": "logo.png"
        },
        "notification": {
            "color": "#03f4fc",
            "thumbnail": "notification.png",
            "description": "${message}"
        },
        "printjob_start_request": {
            "author": "${filename}",
            "color": "#0099ff",
            "buttons": [
                "printjob_request_accept",
                "printjob_request_deny",
                "to_printlist"]
        },
        "printjob_delete_request": {
            "author": "${filename}",
            "color": "#c90000",
            "buttons": [
                "printjob_delete_deny",
                "printjob_delete_accept",
                "to_printlist"]
        },
        "printjob_start":{
            "color": "#25db00",
            "author": "${state.print_stats.filename}",
            "thumbnail": "thumbnail",
            "image": "webcam",
            "buttons": [],
            "fields": [
                {
                    "name": "${embeds.fields.print_left_time}",
                    "value": "${formatTime:time.left}"
                },
                {
                    "name": "${embeds.fields.eta_print_time}",
                    "value": "${formatDate:time.eta}"
                }
            ]
        },
        "printjob_complete":{
            "color": "#25db00",
            "author": "${state.print_stats.filename}",
            "thumbnail": "thumbnail",
            "image": "webcam",
            "buttons": ["clear_file"],
            "fields": [
                {
                    "name": "${embeds.fields.print_time}",
                    "value": "${formatTime:time.duration}"
                }
            ]
        },
        "printjob_pause":{
            "color": "#dbd400",
            "author": "${state.print_stats.filename}",
            "thumbnail": "thumbnail",
            "image": "webcam",
            "buttons": ["printjob_resume","printjob_cancel"],
            "fields": [
                {
                    "name": "${embeds.fields.print_time}",
                    "value": "${formatTime:time.duration}"
                },
                {
                    "name": "${embeds.fields.print_left_time}",
                    "value": "${formatTime:time.left}"
                },
                {
                    "name": "${embeds.fields.print_progress}",
                    "value": "${percent:0:state.display_status.progress}%"
                },
                {
                    "name": "${embeds.fields.print_layers}",
                    "value": "${layers.current}/${layers.top}"
                }
            ]
        },
        "printjob_printing":{
            "color": "#0099ff",
            "author": "${state.print_stats.filename}",
            "thumbnail": "thumbnail",
            "image": "webcam",
            "buttons": [
                "printjob_pause",
                "printjob_cancel",
                "printjob_refresh"],
            "fields": [
                {
                    "name": "${embeds.fields.print_time}",
                    "value": "${formatTime:time.duration}"
                },
                {
                    "name": "${embeds.fields.print_left_time}",
                    "value": "${formatTime:time.left}"
                },
                {
                    "name": "${embeds.fields.eta_print_time}",
                    "value": "${formatDate:time.eta}"
                },
                {
                    "name": "${embeds.fields.print_progress}",
                    "value": "${percent:0:state.display_status.progress}%"
                },
                {
                    "name": "${embeds.fields.print_layers}",
                    "value": "${layers.current}/${layers.top}"
                }
            ]
        },
        "printjob_stop":{
            "color": "#c90000",
            "author": "${state.print_stats.filename}",
            "thumbnail": "thumbnail",
            "image": "webcam",
            "buttons": [],
            "fields": [
                {
                    "name": "${embeds.fields.print_time}",
                    "value": "${formatTime:time.duration}"
                },
                {
                    "name": "${embeds.fields.print_progress}",
                    "value": "${percent:0:state.display_status.progress}%"
                },
                {
                    "name": "${embeds.fields.print_layers}",
                    "value": "${layers.current}/${layers.top}"
                }
            ]
        },
        "temperatures": {
            "color": "#0099ff",
            "thumbnail": "temps.png",
            "buttons": [],
            "fields": [],
            "show_temps": true
        },
        "disconnected":{
            "color": "#c90000",
            "image": "webcam",
