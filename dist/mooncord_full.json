                },
                {
                    "name": "${embeds.fields.total_disk}",
                    "value": "${usage.total_disk} GB"
                },
                {
                    "name": "${embeds.fields.used_disk}",
                    "value": "${usage.used_disk} GB"
                },
                {
                    "name": "${embeds.fields.free_disk}",
                    "value": "${usage.free_disk} GB"
                }
            ]
        },
        "systeminfo_system": {
            "color": "#0099ff",
            "thumbnail": "system.png",
            "selection": {
                "id": "systeminfo_select",
                "data": "${raw_entries}"
