                    "page_down": true
                }
            },
            "printjob_resume": {
                "emoji": "‚ñ∂Ô∏è",
                "style":"PRIMARY",
                "function_mapping": {
                    "required_states" : ["pause"],
                    "macros": ["RESUME"],
                    "message": "${locale.messages.answers.printjob_resume.status_valid}"
                }
            },
            "printjob_cancel": {
                "emoji": "‚õî",
                "style":"DANGER",
                "function_mapping": {
                    "required_states" : ["pause", "printing"],
                    "macros": ["CANCEL_PRINT"],
                    "message": "${locale.messages.answers.printjob_cancel.status_valid}"
                }
            },
            "printjob_pause": {
                "emoji": "‚òï",
                "style":"SECONDARY",
                "function_mapping": {
                    "required_states" : ["printing"],
                    "macros": ["PAUSE"],
                    "message": "${locale.messages.answers.printjob_pause.status_valid}"
                }
            },
            "clear_file": {
                "emoji": "üßπ",
                "style":"SECONDARY",
                "function_mapping": {
                    "required_states" : ["complete"],
                    "macros": ["SDCARD_RESET_FILE"],
                    "message": "${locale.messages.answers.printjob_clear}"
                }
            },
            "printjob_refresh": {
                "emoji": "üîÑ",
                "style":"PRIMARY",
                "function_mapping": {
                    "refresh_status": true
                }
            },
            "to_printlist": {
                "emoji": "üìò",
                "style":"SECONDARY",
                "function_mapping": {
                    "show_printlist": true
                }
            },
            "printjob_delete_accept": {
                "style": "DANGER",
                "function_mapping": {
                    "delete": true,
                    "root_path": "gcodes"
                }
            },
            "printjob_delete_deny": {
                "style":"SUCCESS",
                "function_mapping": {
                    "message": "${locale.messages.answers.printjob_delete_abort}"
                }
            },
            "printjob_start": {
                "emoji": "üñ®Ô∏è",
                "style":"SECONDARY",
                "function_mapping": {
                    "request_printjob": true
                }
            },
            "printjob_delete_request": {
                "emoji": "ü™£",
                "style":"DANGER",
                "function_mapping": {
                    "request_printjob": true,
                    "request_embed": "printjob_delete_request"
                }
            },
            "reconnect": {
                "emoji": "üîÑ",
                "style":"PRIMARY",
                "function_mapping": {
                    "reconnect": true,
                    "message": "${locale.messages.answers.reconnect}"
                }
            },
            "klipper_restart": {
                "emoji": "üîÑ",
                "style":"PRIMARY",
                "function_mapping": {
                    "macros": ["FIRMWARE_RESTART"],
                    "message": "${locale.messages.answers.firmware_restart_successful}"
                }
            },
            "update_system": {
                "style":"SECONDARY",
                "function_mapping": {

                }
            },
            "printjob_request_accept": {
                "style":"SUCCESS",
                "required_states" : ["ready"],
                "function_mapping": {
                    "start_print": true,
                    "message": "${locale.messages.answers.printjob_executed}"
                }
            },
            "printjob_request_deny": {
                "style":"DANGER",
                "function_mapping": {
                    "message": "${locale.messages.answers.printjob_request_abort}"
                }
            }
        },
        "selections": {
            "printlist_view_printjob": {
